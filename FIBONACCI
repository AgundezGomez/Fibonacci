#include <iostream>

#include<iostream>
using namespace std;

int main () {
	int X,Y,Z,Numero,Contador;
	X=0;
	Y=1;
	Z=0;
	cout<< "Cuál número de Fibonacci? ";
	cin>> Numero;
	cout<<"1 ,  ";
	for (Contador=1; Contador<Numero; Contador++){
		Z=X+Y;
		cout<<Z<<" , ";
		X=Y;
		Y=Z;
		
	}
	cin.get();
	return 0;
}
